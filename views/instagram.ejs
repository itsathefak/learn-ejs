<%- include("includes/head.ejs"); %>

    <body>

        <h1>This Account belongs to @<%=userName%>
        </h1>
        <button>Follow</button>
        <button>Message</button>



        <p>Followers :
            <%= data.followers %>
        </p>

        <p>Following :
            <%= data.following %>
        </p>

        <hr>

        <% for(let post of data.posts){ %>
            <img src="<%= post.image %>" alt="some image">
            <p>Likes :
                <%= post.likes %>
            </p>

            <p>Comments :
                <%= post.comments %>
            </p>
            <% } %>

                <%- include("includes/footer.ejs") %>
    </body>

    </html>